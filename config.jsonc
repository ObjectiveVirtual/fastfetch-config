
{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
        "source": "~/.config/fastfetch/os.txt",
        "type": "auto",
        "height": 1,
        "width": 10,
        "padding": {
            "top": 0,
            "left": 1,
            "right": 1
        }
    },
  "modules": [
    {
        "type": "os",
        "key": "🐧 OS",
        "keyColor": "yellow"
    },
    {
        "type": "packages",
        "key": "📦 Packages",
        "keyColor": "blue"
    },
    {
        "type": "uptime",
        "key": "🕰️  Uptime",
        "keyColor": "magenta"
    },
    {
        "type": "command",
        "key": "🧔 OS Age",
        "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days",
        "keyColor": "green"
    },
    {
    	"type": "host",
    	"key": "💻 Device",
    	"keyColor": "red"
    },
    {
    	"type": "terminal",
    	"key": "⌨️  Terminal",
    	"keyColor": "cyan"
    }
  ]
}
